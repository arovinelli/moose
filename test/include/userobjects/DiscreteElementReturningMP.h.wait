#ifndef DISCRETEELEMENTRETURNINGMP_H
#define DISCRETEELEMENTRETURNINGMP_H

#include "DiscreteElementUserObject.h"

class DiscreteElementReturningMP;

template <>
InputParameters validParams<DiscreteElementReturningMP>();

/**
 * Crystal plasticity system userobject base class.
 */
class DiscreteElementReturningMP : public DiscreteElementUserObject
{
public:
  /// Returns Material Properties Sizes
  virtual std::vector<unsigned int> getMaterialPropertySizes() const;

  /// Returns the Material Properties Names
  virtual std::vector<std::string> getMaterialPropertyNames() const;

protected:
  const std::vector<std::string> _MP_names;
  const std::vector<unsigned int> _MP_sizes;
};

#endif // DISCRETEELEMENTRETURNINGMP_H
